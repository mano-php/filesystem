{
    "name": "mano-code/filesystem",
    "alias": "文件系统",
    "description": "支持 本地 OSS COS 七牛云，文件上传、线上预览、程序调用、等功能。",
    "type": "library",
    "version": "1.0.0",
    "keywords": [ "extension"],
    "homepage": "https://github.com/mano-php/filesystem",
    "license": "MIT",
    "authors": [
        {
            "name": "Mano",
            "email": "ManoCode@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "slowlyo/owl-admin": "*",
        "slowlyo/owl-dict": "*",
        "iidestiny/laravel-filesystem-oss": "3.3",
        "overtrue/laravel-filesystem-cos": "3.5.0",
        "zgldh/qiniu-laravel-storage": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mano\\FileSystem\\": "src/"
        }
    },
    "extra": {
        "owl-admin": "Mano\\FileSystem\\FilesystemServiceProvider",
        "laravel": {
            "providers": [
                "Mano\\FileSystem\\FilesystemServiceProvider"
            ]
        }
    }
}
